{
  "root": true,
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "globals": {
    "IS_DEVELOPMENT": "readonly"
  },

  "parserOptions": {
    "ecmasVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["html", "react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "es2021": true
  }
}
